{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es3.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "globals": {"S": "readonly", "em": "readonly", "search": "readonly"},
      "rules": {
        "func-style": ["error", "declaration", {"allowArrowFunctions": true}],
        "no-unused-vars": ["error", {"args": "none", "varsIgnorePattern": "^em$"}]
      }
    }
  ]
}
